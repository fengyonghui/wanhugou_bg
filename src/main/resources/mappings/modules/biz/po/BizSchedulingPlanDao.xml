	<insert id="insert" useGeneratedKeys="true" keyProperty="id">
		INSERT INTO biz_scheduling_plan(
			id,
			object_name,
			object_id,
			original_num,
			<!--<if test="schedulingNum !=null and schedulingNum !=''">-->
				<!--scheduling_num,-->
			<!--</if>-->
			plan_date,
			status,
			create_id,
			create_time,
			u_version,
			update_id,
			update_time
		) VALUES (
			#{id},
			#{objectName},
			#{objectId},
			#{originalNum},
			<!--<if test="schedulingNum !=null and schedulingNum !=''">-->
				<!--#{schedulingNum},-->
			<!--</if>-->
			#{planDate},
			#{delFlag},
			#{createBy.id},
			#{createDate},
			#{uVersion},
			#{updateBy.id},
			#{updateDate}
		)
	</insert>
	
	<update id="update">
		UPDATE biz_scheduling_plan SET 	
			object_name = #{objectName},
			object_id = #{objectId},
			original_num = #{originalNum},
-- 			scheduling_num = #{schedulingNum},
			<!--complete_num = #{completeNum},-->
			create_time = #{createDate},
			u_version = #{uVersion},
			update_id = #{updateBy.id},
			update_time = #{updateDate}
		WHERE id = #{id}
	</update>
	
	<update id="delete">
		UPDATE biz_scheduling_plan SET 
			status = #{DEL_FLAG_DELETE}
		WHERE id = #{id}
	</update>

